<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.krawler.esp.hibernate.impl.mb_configmasterdata" table="mb_configmasterdata">
    <id column="id" name="masterid" type="string">
      <generator class="uuid"/>
    </id>
    <property name="configid">
      <column name="configid"/>
    </property>
    <property name="masterdata">
      <column name="masterdata"/>
    </property>
  </class>
</hibernate-mapping>
